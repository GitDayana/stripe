  <article id="built-for-developers">
    <header>
      <h2>Built for developers</h2>
      <p>Robust, clean APIs that let you focus on building great products.</p>
    </header>

    <div class="examples inner-col smaller">

<script type='text/template' class='curl'><span class="hljs-keyword">curl</span> <span style="color:rgba(34, 135, 34, 1)">https://api.stripe.com/v1/charges</span> \
  -u <span class="hljs-string">sk_test_BQokikJOvBiI2HlWgH4olfQ2</span>: \
  -d <span class="hljs-symbol">amount</span>=<span class="hljs-string">400</span> \
  -d <span class="hljs-symbol">currency</span>=<span class="hljs-string">usd</span> \
  -d "<span class="hljs-symbol">description</span>=<span class="hljs-string">Charge for test@example.com</span>" \
  -d "<span class="hljs-symbol">source[object]</span>=<span class="hljs-string">card</span>" \
  -d "<span class="hljs-symbol">source[number]</span>=<span class="hljs-string">4242424242424242</span>" \
  -d "<span class="hljs-symbol">source[exp_month]</span>=<span class="hljs-string">12</span>" \
  -d "<span class="hljs-symbol">source[exp_year]</span>=<span class="hljs-string">2017</span>" \
  -d "<span class="hljs-symbol">source[cvc]</span>=<span class="hljs-string">123</span>"</script>

      <script type="text/javascript">
        window.APIValues = {};

          APIValues["ruby"] = "\u003Cpre\u003E\u003Ccode class=\"language-ruby\"\u003Erequire \u0026quot;stripe\u0026quot;\nStripe.api_key = \u0026quot;sk_test_BQokikJOvBiI2HlWgH4olfQ2\u0026quot;\n\nStripe::Charge.create(\n  :amount =\u0026gt; 400,\n  :currency =\u0026gt; \u0026quot;usd\u0026quot;,\n  :source =\u0026gt; {\n    :number =\u0026gt; \u0026quot;4242424242424242\u0026quot;,\n    :exp_month =\u0026gt; 3,\n    :exp_year =\u0026gt; 2017,\n    :cvc =\u0026gt; \u0026quot;314\u0026quot;\n  },\n  :description =\u0026gt; \u0026quot;Charge for test@example.com\u0026quot;\n)\u003C/code\u003E\u003C/pre\u003E";
          APIValues["python"] = "\u003Cpre\u003E\u003Ccode class=\"language-python\"\u003Eimport stripe\nstripe.api_key = \u0026quot;sk_test_BQokikJOvBiI2HlWgH4olfQ2\u0026quot;\n\nstripe.Charge.create(\n  amount=400,\n  currency=\u0026quot;usd\u0026quot;,\n  source={\n    \u0026quot;number\u0026quot;: \u0026#39;4242424242424242\u0026#39;,\n    \u0026quot;exp_month\u0026quot;: 12,\n    \u0026quot;exp_year\u0026quot;: 2017,\n    \u0026quot;cvc\u0026quot;: \u0026#39;123\u0026#39;\n  },\n  description=\u0026quot;Charge for test@example.com\u0026quot;\n)\u003C/code\u003E\u003C/pre\u003E";
          APIValues["php"] = "\u003Cpre\u003E\u003Ccode class=\"language-php\"\u003E\\Stripe\\Stripe::setApiKey(\u0026quot;sk_test_BQokikJOvBiI2HlWgH4olfQ2\u0026quot;);\n\n\\Stripe\\Charge::create(array(\n  \u0026quot;amount\u0026quot; =\u0026gt; 400,\n  \u0026quot;currency\u0026quot; =\u0026gt; \u0026quot;usd\u0026quot;,\n  \u0026quot;source\u0026quot; =\u0026gt; array(\n    \u0026quot;number\u0026quot; =\u0026gt; \u0026quot;4242424242424242\u0026quot;,\n    \u0026quot;exp_month\u0026quot; =\u0026gt; 3,\n    \u0026quot;exp_year\u0026quot; =\u0026gt; 2017,\n    \u0026quot;cvc\u0026quot; =\u0026gt; \u0026quot;314\u0026quot;\n  ),\n  \u0026quot;description\u0026quot; =\u0026gt; \u0026quot;Charge for test@example.com\u0026quot;\n));\u003C/code\u003E\u003C/pre\u003E";
          APIValues["java"] = "\u003Cpre\u003E\u003Ccode class=\"language-java\"\u003EStripe.apiKey = \u0026quot;sk_test_BQokikJOvBiI2HlWgH4olfQ2\u0026quot;;\n\nMap\u0026lt;String, Object\u0026gt; chargeParams = new HashMap\u0026lt;String, Object\u0026gt;();\nchargeParams.put(\u0026quot;amount\u0026quot;, 400);\nchargeParams.put(\u0026quot;currency\u0026quot;, \u0026quot;usd\u0026quot;);\nMap\u0026lt;String, Object\u0026gt; sourceParams = new HashMap\u0026lt;String, Object\u0026gt;();\nsourceParams.put(\u0026quot;number\u0026quot;, \u0026quot;4242424242424242\u0026quot;);\nsourceParams.put(\u0026quot;exp_month\u0026quot;, 3);\nsourceParams.put(\u0026quot;exp_year\u0026quot;, 2017);\nsourceParams.put(\u0026quot;cvc\u0026quot;, \u0026quot;314\u0026quot;);\nchargeParams.put(\u0026quot;source\u0026quot;, sourceParams);\nchargeParams.put(\u0026quot;description\u0026quot;, \u0026quot;Charge for test@example.com\u0026quot;);\n\nCharge.create(chargeParams);\u003C/code\u003E\u003C/pre\u003E";
          APIValues["node"] = "\u003Cpre\u003E\u003Ccode class=\"language-javascript\"\u003Evar stripe = require(\u0026quot;stripe\u0026quot;)(\n  \u0026quot;sk_test_BQokikJOvBiI2HlWgH4olfQ2\u0026quot;\n);\n\nstripe.charges.create({\n  amount: 400,\n  currency: \u0026quot;usd\u0026quot;,\n  source: {\n    number: \u0026#39;4242424242424242\u0026#39;,\n    exp_month: 12,\n    exp_year: 2017,\n    cvc: \u0026#39;123\u0026#39;\n  },\n  description: \u0026quot;Charge for test@example.com\u0026quot;\n}, function(err, charge) {\n  // asynchronously called\n});\u003C/code\u003E\u003C/pre\u003E";
          APIValues["go"] = "\u003Cpre\u003E\u003Ccode class=\"language-go\"\u003Estripe.Key = \u0026quot;sk_test_BQokikJOvBiI2HlWgH4olfQ2\u0026quot;\n\nchargeParams := \u0026amp;stripe.ChargeParams{\n  Amount: 400,\n  Currency: \u0026quot;usd\u0026quot;,\n  Desc: \u0026quot;Charge for test@example.com\u0026quot;,\n}\nchargeParams.SetSource(\u0026amp;stripe.CardParams{\n        Number: \u0026quot;4242424242424242\u0026quot;,\n        Month:  \u0026quot;12\u0026quot;,\n        Year:   \u0026quot;2017\u0026quot;,\n        CVC:    \u0026quot;123\u0026quot;,\n    })\n\nch, err := charge.New(chargeParams)\u003C/code\u003E\u003C/pre\u003E";
      </script>

