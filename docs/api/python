      &quot;migrated_to&quot;: null,
      &quot;name&quot;: &quot;John Doe&quot;,
      &quot;type&quot;: &quot;individual&quot;,
      &quot;verified&quot;: false
    },
    #&lt;com.stripe.model.Recipient[...] ...&gt;,
