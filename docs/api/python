  &quot;object&quot;: &quot;list&quot;,
  &quot;url&quot;: &quot;/v1/refunds&quot;,
  &quot;has_more&quot;: false,
  &quot;data&quot;: [
    #&lt;Stripe::Refund id=re_17uV7O2eZvKYlo2C0lPUUdBc 0x00000a&gt; JSON: {
