  &quot;fee_8AbwhjTltZ8Rxp&quot;,
  &quot;fr_8AqzITL01NuCc5&quot;,
  function(err, refund) {
    // asynchronously called
  }
);</code></pre></span><span class="lang lang-go"><pre><code class="language-go">stripe.Key = &quot;sk_test_BQokikJOvBiI2HlWgH4olfQ2&quot;

