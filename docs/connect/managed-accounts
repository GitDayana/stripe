  <script type="text/javascript">
      var APIValues = {};
          APIValues["curl"] = {
            tutorial_managed: "\u003Cpre class=\"language-bash\"\u003E\u003Ccode class=\"language-bash\"\u003Ecurl \u003Cspan class='token string'\u003Ehttps://api.stripe.com/v1/accounts\u003C/span\u003E \\\n   -u \u003Cspan class='token string'\u003E{PLATFORM_SECRET_KEY}:\u003C/span\u003E \\\n   -d \u003Cspan class='token keyword'\u003Ecountry\u003C/span\u003E=\u003Cspan class='token string'\u003EUS\u003C/span\u003E \\\n   -d \u003Cspan class='token keyword'\u003Emanaged\u003C/span\u003E=\u003Cspan class='token string'\u003Etrue\u003C/span\u003E\n\u003C/code\u003E\u003C/pre\u003E"
          };
          APIValues["ruby"] = {
            tutorial_managed: "\u003Cspan class=\"highlight_js ruby\"\u003EStripe.api_key = PLATFORM_SECRET_KEY\nStripe::Account.create(\n  {\n    :country =\u0026gt; \u0026quot;US\u0026quot;,\n    :managed =\u0026gt; true\n  }\n)\u003C/span\u003E"
